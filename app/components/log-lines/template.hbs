{{#if log}}
  <div class="ui inverted segment" data-autoid="log">
    {{#each lines as |line|}}
      <div>{{line}}</div>
    {{/each}}
  </div>
{{else}}
  <div class="ui warning message">
    <div class="header" data-autoid="message">
      Got null response from API. Maybe Pod is in Pending phase?
    </div>
  </div>
{{/if}}
